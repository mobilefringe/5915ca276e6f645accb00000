<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var restaurants = getStoresListByCategoryID(5119);
            renderStoreList('#store_list_container','#store_list_template', restaurants, "stores");
            
            show_content();
            
            store_search();
            show_cat_stores();
            $('#top_nav_stores').addClass('active_yellow')
        }
        loadMallDataCached(renderAll); 
    });
</script>